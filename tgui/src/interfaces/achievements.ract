<ui-display>
	{{#each data.categories}}
		<ui-section label='{{category_name}}'>
			{{#each achievements}}
				<ui-section label='{{name}}'>
					{{desc}}<br>
					{{#if unlocked}}
						<span class='good'>Unlocked</span>
					{{else}}
						<span class='bad'>locked</span>
					{{/if}}
				</ui-section>
			{{/each}}
		</ui-section>
	{{/each}}
</ui-display>
